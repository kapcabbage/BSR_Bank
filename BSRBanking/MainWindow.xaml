<Window x:Class="BSRBanking.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BSRBanking"
        mc:Ignorable="d"
        Title="{Binding Title}" 
        Height="350" Width="550"
        DataContext="{Binding Main, Source={StaticResource Locator}}" ResizeMode="NoResize">
    <Grid>
        <Frame Name="_mainFrame" Source="Views/LoginPage.xaml" Margin="10" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
